package primeiro.progeto;

public class IfEncadeado {

	public static void main(String[] args) {
		
		boolean opcao1 = true;
		boolean opcao2 = true;
		boolean opcao3 = true;
		
		if (opcao1) {
			System.out.println("Entrou na opção 1 ");
			
		} else {
			System.out.println("Não entrou na opção 1 ");
			
		}

	}

}
